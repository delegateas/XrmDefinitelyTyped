{
    "typescript.tsdk": "./node_modules/typescript/lib",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "build": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.js.map": {"when": "$(basename).ts"}
    }
}